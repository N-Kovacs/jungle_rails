<h1>Login</h1>
<div class="row">
<div class="col-sm-6 col-sm-offset-1">
<%= form_tag '/login' do %>
  <div class="form-group">
  Email: <%= text_field_tag :email %>
  <div class="form-group">
  Password: <%= password_field_tag :password %>
  <div class="form-group">
  <%= submit_tag "Submit" %>
  </div>
  </div>
  </div>

<% end %>
</div>
</div>